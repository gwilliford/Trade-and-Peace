load(file = "./models/atgap.Rdata")
atgap1_t <- tvtable.tvcure(atgap1, format = "long")
atgap2_t <- tvtable.tvcure(atgap2, format = "long")
atgap3_t <- tvtable.tvcure(atgap3, format = "long")
atgap4_t <- tvtable.tvcure(atgap4, format = "long")
atgap_table <- tvtable_combine(c("atgap1_t", "atgap2_t", "atgap3_t", "atgap4_t"), format = "long", footnote = "text")
x <- tvtable_xtable(atgap_table, caption = "Peaceful Settlement Attempts")
printer(x, file = "./tables/atgap_table.tex", tabular.enviroment = "tabularx", booktabs = T)




