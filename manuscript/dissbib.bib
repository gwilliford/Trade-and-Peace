@article{dinar2009,
  title = {Scarcity and {{Cooperation Along International Rivers}}},
  author = {Dinar, Shlomi},
  date = {2009-02},
  journaltitle = {Global Environmental Politics},
  shortjournal = {Global Environmental Politics},
  volume = {9},
