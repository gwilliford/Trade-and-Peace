@article{megoran2006,
  title = {For Ethnography in Political Geography: {{Experiencing}} and Re-Imagining {{Ferghana Valley}} Boundary Closures},
  shorttitle = {For Ethnography in Political Geography},
  author = {Megoran, Nick},
  date = {2006-08-01},
  journaltitle = {Political Geography},
